        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	
	
	gs.setProperty('x_caci_cisco_inter.intersight_log_verbosity', current.log_levels);
	gs.setProperty('x_caci_cisco_inter.cisco.intersight.support', current.support_url);
	gs.setProperty('x_caci_cisco_inter.Cisco_Incident_Handler', current.incident_assignment_group);
	gs.setProperty('x_caci_cisco_inter.IncidentCallerName', current.incident_caller_name);
		
	if(current.advanced && current.conditions!=''){
		gs.setProperty('x_caci_cisco_inter.alarm.raise.incident.conditions', current.conditions);
	}else if(current.advanced+''=='false' && current.incident_severity){
		gs.setProperty('x_caci_cisco_inter.alarm.raise.incident.conditions', "severityIN"+current.incident_severity);
	}else{
		gs.setProperty('x_caci_cisco_inter.alarm.raise.incident.conditions', "severityIN");
	}
	
})(current, previous);]]></script>
